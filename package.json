{
  "version": "0.0.0",
  "author": "Oliver Dudgeon",
  "name": "@squonk/openapi-js-client-generator",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/InformaticsMatters/openapi-js-client-generator"
  },
  "description": "TypeScript client for the Squonk OpenAPI files. Generated from the Open API spec with orval. Provides React Hooks to fetch data with react-query using Axios under-the-hood.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "orval --config ./orval.config.cjs && tsup && node setup-entrypoints.js",
    "orval": "orval --config ./orval.config.cjs"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "axios": "^0.24.0",
    "eslint": "^8.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "orval": "^6.3.0",
    "prettier": "^2.4.1",
    "react-query": "^3.32.3",
    "tslib": "^2.3.1",
    "tsup": "^5.7.0",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "axios": ">=0.23",
    "react-query": ">=3"
  }
}
